{
  "name": "json2csv",
  "version": "1.0.0",
  "description": "Backend that converts .JSON files into .CSV files",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "dev": "nodemon"
  },
  "keywords": [
    "conversion",
    "JSON",
    "CSV"
  ],
  "author": "Khaan Claasz Coockson",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
